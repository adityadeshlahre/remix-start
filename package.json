{
	"name": "remix-start",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "remix vite:dev",
		"lint": "npx @biomejs/biome check --write .",
		"format": "npx @biomejs/biome format",
		"setup": "npx prisma generate && npx prisma migrate deploy && npx prisma db seed",
		"start": "NODE_ENV=production remix-serve build/server/index.js",
		"typecheck": "tsc",
		"clean": "rimraf ./build ./node_modules ./pnpm-lock.yaml",
		"postinstall": "npx prisma generate --schema=./prisma/schema.prisma"
	},
	"dependencies": {
		"@prisma/client": "^6.0.1",
		"@remix-run/dev": "^2.15.1",
		"@remix-run/node": "^2.15.1",
		"@remix-run/react": "^2.15.1",
		"@remix-run/serve": "^2.15.1",
		"isbot": "^5.1.17",
		"match-sorter": "^8.0.0",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"remix": "^2.15.1",
		"sort-by": "^1.2.0",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@remix-run/dev": "^2.15.0",
		"@rollup/rollup-linux-x64-musl": "^4.28.1",
		"@types/node": "^22.10.1",
		"@types/react": "^19.0.1",
		"@types/react-dom": "^19.0.2",
		"autoprefixer": "^10.4.20",
		"postcss": "^8.4.49",
		"prisma": "^6.0.1",
		"rimraf": "^6.0.1",
		"tailwindcss": "^3.4.16",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"prisma": {
		"seed": "node --loader ts-node/esm ./prisma/seed.ts"
	}
}
